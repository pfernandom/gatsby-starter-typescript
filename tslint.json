{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [ "never-prefix" ],
        "semicolon": [false, "never"],
        "member-access": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "no-console": [
            false
        ],
        "no-var-requires": false,
        "quotemark": [true, "single", "jsx-double"]
    },
    "rulesDirectory": []
}